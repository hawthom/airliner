- var pluginName = query.pluginName;
table.table.table-condensed
  tbody
    tr
      th Commands Accepted 
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/cmdAcceptCount'}]}) ---
      td
    tr
      th Commands Rejected
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/cmdRejectCount'}]}) ---
      td
    tr
      th Messages Received
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/msgRecvCount'}]}) ---
      td
    tr
      th Parameters Parsed
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/paramsParsed'}]}) ---
      td
    tr
      th Binary Input Stream
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/binaryInputStreamID'}]}) ---
      td
    tr
      th JSON Output Stream
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/jsonOutputStreamID'}]}) ---
      td
    tr
      th Telemetry Def Req Stream
      td
        div(data-cdr={tlm:[{name:'/' + pluginName + '/hk/tlmDefReqStreamID'}]}) ---
      td

              
footer.btn-group(role="group")       
  button.btn.cdr-primary(data-cdr={cmd:{name:'/' + pluginName + '/reset'}}) Reset